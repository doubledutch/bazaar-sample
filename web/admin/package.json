{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.1.0",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "@horizon/client": "^2.0.0",
    "bazaar-client": "^0.40.22",
    "less-watch-compiler": "^1.4.2",
    "lodash": "^4.17.4",
    "prepack": "^0.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-shallow-compare": "^15.5.2",
    "react-addons-update": "^15.4.2",
    "react-autobind": "^1.0.6",
    "react-scripts": "^0.8.5"
  },
  "scripts": {
    "start": "concurrently \"npm run start-less\" \"react-scripts start\"",
    "build": "npm run build-less && react-scripts build && npm run build-static",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-less": "node_modules/less-watch-compiler/less-watch-compiler.js style/ public/style/ index.less",
    "build-less": "lessc style/index.less -x build/static/css/index.css",
    "build-static": "cp public/static/* build/static/"
  }
}