{
  "name": "bazaar_sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --port 8081",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "web": "webpack-dev-server --port 3000 --config web/webpack.config.dev.js -d --inline --hot --colors",
    "clean-web": "rm -Rf web/static/build/",
    "build-web": "npm run clean-web && webpack --colors --display-error-details --config web/webpack.config.prod.js"
  },
  "dependencies": {
    "@horizon/client": "^2.0.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-runtime": "^6.20.0",
    "bazaar-client": "0.40.0",
    "json-loader": "^0.5.4",
    "react-native-web": "0.0.68",
    "soap": "^0.18.0",
    "webpack": "^1.14.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "prepack": "^0.1.1",
    "react": "15.4.1",
    "react-addons-update": "15.4.1",
    "react-native": "0.40.0",
    "react-native-cli": "^2.0.1"
  }
}